{
    "$schema": "https://schema.tauri.app/config/2/capability",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "shell:allow-open",
        "http:default",
        {
            "identifier": "http:allow-fetch",
            "allow": [
                {
                    "url": "http://*"
                },
                {
                    "url": "https://*"
                }
            ]
        }
    ]
}